{
  "name": "devolve-ui-wasm",
  "version": "1.0.0",
  "description": "TypeScript interface and WebAssembly build of devolve-ui, so you can use it in node.js and the browser",
  "type": "module",
  "main": "dist/js/index.js",
  "types": "dist/types/index.d.ts",
  "typesVersions": {
    "*": {
      "jsx-runtime": [
        "dist/types/jsx-runtime.d.ts"
      ]
    }
  },
  "scripts": {
    "typegen": "tsc",
    "typegen-watch": "tsc --watch",
    "codegen": "node esbuild.cjs",
    "codegen-watch": "node esbuild.cjs --watch",
    "build": "run-s typegen codegen",
    "watch": "run-p typegen-watch codegen-watch",
    "lint": "node ts-standardx.mjs",
    "lint-fix": "node ts-standardx.mjs --fix",
    "real-test": "echo 'No tests yet'",
    "test": "run-s lint real-test",
    "prepare": "husky install"
  },
  "keywords": [
    "devolve-ui",
    "ui"
  ],
  "author": "Jakobeha",
  "license": "Apache-2.0",
  "devDependencies": {
    "@typescript-eslint/parser": "^4.0.0",
    "colorette": "^2.0.19",
    "esbuild": "^0.14.51",
    "esbuild-plugin-wasm": "^1.0.0",
    "eslint": "^7.0.0",
    "husky": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "ts-standard": "^11.0.0",
    "typescript": "^4.7.4"
  }
}
